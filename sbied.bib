}

@Article{Schmitt1999,
  author           = {Schmitt, R. J. and Holbrook, S. J. and Osenberg, C. W.},
  journal          = {Ecol Lett},
  title            = {Quantifying the effects of multiple processes on local abundance: a cohort approach for open populations},
  year             = {1999},
  pages            = {294--303},
  volume           = {2},
  abstract         = {A challenge for species with demographically open populations is to evaluate the relative importance of various processes that together set local abundance. We developed a cohort-based framework for quantifying the influence of an external supply of colonists and subsequent density-independent and density-dependent mortality on local abundance. Two complementary approaches --- based on limitation and elasticity --- revealed the nature of interactions and nonlinearities among these processes. Data for an Indo-Pacific reef fish were used to document the settler-survivor relationship and to quantify natural variation in settlement. Limitation by density-dependence was two-fold and 20-fold greater than by supply or density-independent mortality, respectively. Elasticity analyses showed that adult abundance was 40% more sensitive to small proportionate changes in supply than in density-dependence. These techniques provide a way to compare across systems, which could enhance our ability to draw general conclusions regarding the processes that shape local abundance of species with open populations.},
  creationdate     = {2019-06-05T00:00:00},
  doi              = {10.1046/j.1461-0248.1999.00086.x},
  fjournal         = {Ecology Letters},
  modificationdate = {2023-08-09T08:16:56},
  owner            = {kingaa},
}

@Article{Schnute1994,
  author           = {Schnute, J. T.},
  journal          = {Can J Fish Aquat Sci},
  title            = {A General Framework For Developing Sequential Fisheries Models},
  year             = {1994},
  month            = aug,
  number           = {8},
  pages            = {1676--1688},
  volume           = {51},
  abstract         = {A sequential fisheries model relates observed data to the biological dynamics of an underlying stock. Either model component, dynamic or observational, can be subject to statistical variation. Current fisheries literature includes models with (1) variable dynamics and no observation error, (2) deterministic dynamics and observations subject to measurement error, and (3) combined dynamic and measurement variability. This paper presents a general framework for developing sequential fishery models and estimating model parameters from available data. The framework encompasses most traditional stock assessment models and suggests new, potentially useful extensions. It generalizes the conventional definition of state space model to include nonlinear equations with nonnormal error. The paper rigorously compares two paradigms (KF: Kalman filter, EV: errors in variables) used for parameter estimation. Each paradigm is formulated in both frequentist and Bayes contexts, where Bayes is shown to be most appropriate for the EV paradigm. Model design concepts are illustrated with a simple example oriented to catch data and a more complex example with catch-at-age data. Because the framework forces essential questions to be asked about underlying processes, observed data, and sources of variability, it can help delineate the limits of current knowledge and establish rational priorities for future data collection.},
  creationdate     = {2019-06-05T00:00:00},
  fjournal         = {Canadian Journal of Fisheries and Aquatic Sciences},
  j9               = {CAN J FISHERIES AQUAT SCI},
  ji               = {Can. J. Fish. Aquat. Sci.},
  keywords         = {SAMPLING-RESAMPLING PERSPECTIVE; BAYESIAN STATISTICS; STRUCTURED MODEL; FISH POPULATION; KALMAN FILTER; AGE DATA; CATCH; RECRUITMENT; ERRORS; TEARS},
  modificationdate = {2023-08-09T08:16:56},
  owner            = {kingaa},
  pdf              = {Schnute1994.pdf\:Schnute1994.pdf\:PDF:Schnute1994.pdf\:Schnute1994.pdf\:PDF:PDF},
  sn               = {0706-652X},
  tc               = {56},
  ut               = {ISI:A1994PW57100001},
}

@Article{Schnute1995,
  author           = {Schnute, J. T. and Richards, L. J.},
  journal          = {Can J Fish Aquat Sci},
  title            = {The influence of error on population estimates from catch-age models},
  year             = {1995},
  month            = oct,
  number           = {10},
  pages            = {2063--2077},
  volume           = {52},
  abstract         = {Catch-age analysis has become a standard tool for modern stock assessment, where it is recognized that population estimates are linked to estimates of the natural mortality rate M. We use state space models and simulation-estimation cycles to evaluate the performance of catch-age analysis. In particular, we design a simulation model with realistic measurement and process error. We then examine how effectively unknown parameters and population sizes can be obtained from an estimation model simplified to conform with common assumptions. We describe circumstances in which, simultaneously with all other parameters, M is estimable from traditional catch-age data. Increased fishing contrast improves estimates of some model parameters, but not M. The variance ratio rho relating process error in recruitments with measurement error in the abundance index must be specified in the estimation. We consider both true and ad hoc choices for rho. In general, estimation models perform better when the true rho is specified and measurement error is low, so that recruitment error is the dominant source of model variability. Thus, well-designed data collection programs are critical to the correct interpretation of actual fishery data.},
  creationdate     = {2007-07-03T00:00:00},
  fjournal         = {Canadian Journal of Fisheries and Aquatic Sciences},
