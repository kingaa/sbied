}

@Article{Rohani2010,
  author           = {Rohani, Pejman and King, Aaron A.},
  journal          = {Trends Ecol Evol},
  title            = {Never mind the length, feel the quality: the impact of long-term epidemiological data sets on theory, application and policy},
